<templateSet group="Java">
  <template name="mtodo" value="//    TODO $Date$ $Time$: " description="TODO with Date and Time" toReformat="true" toShortenFQNames="true">
    <variable name="Date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="Time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="mfixme" value="//    FIXME $Date$ $Time$: " description="FIXME with date and time" toReformat="true" toShortenFQNames="true">
    <variable name="Date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="Time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="puvm" value="public void $END$(){&#10;&#10;}" description="public void method" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="prvm" value="private void $END$ (){&#10;&#10;}" description="private void method" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>